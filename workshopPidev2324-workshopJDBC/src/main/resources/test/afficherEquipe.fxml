<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane fx:id="mainroot"
          style="-fx-background-color: #033E6A;"
          xmlns="http://javafx.com/javafx/21"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="test.AfficherController"
          hgap="10">

            <AnchorPane layoutY="-5.0"
                        prefHeight="155.0"
                        prefWidth="1283.0"
                        GridPane.rowIndex="0"
                        GridPane.columnIndex="0"
                        GridPane.columnSpan="4"
                        style="-fx-background-color: #C4CCD1;-fx-background-radius: 20 20 0 0;">
                <Text fill="#033e6a" layoutX="33.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="Afficher les équipes" textAlignment="CENTER">
                    <font>
                        <Font name="Roboto Bold" size="35.0" />
                    </font>
                </Text>
            </AnchorPane>
            <GridPane GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.columnSpan="1">

            </GridPane>
            <ScrollPane fx:id="scrollPane"
                        GridPane.rowIndex="1"
                        GridPane.columnSpan="1"
                        GridPane.columnIndex="1">
                <content>
                </content>
            </ScrollPane>
            <Button fx:id="btnAjouterEquipe"
                    contentDisplay="CENTER"
                    layoutX="1058.0"
                    layoutY="641.0"
                    mnemonicParsing="false"
                    onAction="#Ajouter"
                    prefWidth="200.0"
                    GridPane.rowIndex="2"
                    GridPane.columnIndex="1"
                    style="-fx-background-color: #033e6a; -fx-font-weight: bold; -fx-background-radius: 5; -fx-padding: 8;" text="Ajouter Equipe" textAlignment="CENTER" textFill="WHITE">
                <font>
                    <Font size="15.0" />
                </font>
            </Button>
        <columnConstraints>
            <ColumnConstraints percentWidth="20"/>
            <ColumnConstraints percentWidth="80"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints percentHeight="20"/>
            <RowConstraints percentHeight="70"/>
            <RowConstraints percentHeight="10"/>
        </rowConstraints>

            




</GridPane>
